{
    "llvm-target": "wasm32-unknown-unknown",
    "cpu": "generic",
    "features": "+mutable-globals,+nontrapping-fptoint,+sign-ext,+bulk-memory",
    "build-tags": [
        "tinygo.wasm",
        "wasm_unknown"
    ],
    "goos": "linux",
    "goarch": "arm",
    "linker": "wasm-ld",
    "rtlib": "compiler-rt",
    "scheduler": "none",
    "cflags": [
        "-mno-bulk-memory",
        "-mnontrapping-fptoint",
        "-msign-ext"
    ],
    "ldflags": [
        "--allow-undefined",
        "--no-demangle",
        "--import-memory",
        "--initial-memory=65536",
        "--max-memory=65536",
        "--stack-first",
        "--no-entry",
        "-zstack-size=14752"
    ]
}
